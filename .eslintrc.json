{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "consistent-return": ["warn"],
    "no-underscore-dangle": ["warn"]
  },
  "plugins": [
    "prettier",
    "html"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
